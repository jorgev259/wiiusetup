{
    "Homebrew App Store":{
        "type":"github",
        "repo":"vgmoose/hbas",
        "steps":[
            {
                "type": "extractFile",
                "file": ".zip",
                "path":"wiiu"
            }
        ]
    },

    "WUP Installer GX2 _HBL_":{
        "type":"direct",
        "url":"http://wiiubru.com/appstore/zips/wup_installer_gx2.zip",
        "steps":[
            {
                "type": "extractFile",
                "path":""
            }
        ]
    },

    "WUP Installer GX2 _Channel_":{
        "type":"direct",
        "url":"http://www.wiiubru.com/appstore/chan_zips/wup_installer_gx2.zip",
        "steps":[
            {
                "type": "extractFile",
                "path": "install"
            }
        ]
    },

     "HBL 1.3":{
        "type":"github",
        "repo":"dimok789/homebrew_launcher",
         "version":"v1.3",
        "steps":[
            {
                "type": "extractFile",
                "file": "homebrew_launcher.v1.3.zip",
                "path": ""
            }
        ]
    },

    "Haxchi":{
        "type":"github",
        "repo": "FIX94/haxchi",
        "steps":[
            {
                "type": "extractFile",
                "file": "Haxchi",
                "fileDelete":"haxchi/config.txt",
                "path": ""
            },
            {
                "type": "extractFile",
                "file": "CBHC",
                "path": ""
            }
        ]
    },

    "Config.txt":{
        "type":"direct",
        "url":"https://wiiu.guide/images/config.txt",
        "steps":[
            {
                "type":"addFile",
                "file":"config.txt",
                "path":"haxchi"
            }
        ]
    },

    "HBL Channel":{
        "type":"github",
        "repo":"dimok789/homebrew_launcher",
        "steps":[
            {
                "type": "extractFile",
                "file":".zip",
                "path": "install/hbc"
            }
        ]
    },

    "Mocha CFW":{
        "type":"github",
        "repo":"dimok789/mocha",
        "version":"",
        "steps":[
            {
                "type":"extractFile",
                "file":".zip",
                "fileExtract":"mocha.elf",
                "path":"wiiu/apps/mocha"
            }
        ]
    },

