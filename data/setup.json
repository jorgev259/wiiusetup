    "SDCafiine":{
        "type":"github",
        "repo":"Maschell/SDCafiine",
        "steps":[
            {
                "type": "extractFile",
                "file":".zip",
                "path": ""
            },
            {
                "type":"folder",
                "name":"sdcafiine/exampleID(one modpack)/content/patch"
            },
            {
                "type":"folder",
                "name":"sdcafiine/exampleID(multiple modpacks)/modpack1/content/patch"
            },
            {
                "type":"folder",
                "name":"sdcafiine/exampleID(multiple modpacks)/modpack2/content/patch"
            }
        ]
